program soalno3;
uses crt;


var
v :Integer;
i ,hasil:real;



begin
writeln('soal3');
WriteLn('hambatan sebuah toaster');
WriteLn('menghitung hambatan');

v:=110;
i:=2.8;
  
  hasil:= v/i;
  writeln('hasil =', hasil:1:2, ' A');
  
