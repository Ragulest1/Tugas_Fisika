program soal4;
uses crt;


var
v :Integer;
i ,r :Real;



begin
  writeln('soal3');
WriteLn('hambatan sebuah toaster');
WriteLn('menghitung hambatan');

v:=110;
i:=5.5;
  
  r:= v/i;
  writeln('r =', r:1:0, ' ohm');
  writeln('soal3');
WriteLn('hambatan sebuah toaster');
WriteLn('menghitung hambatan');

writeln('hasil perhitungan diatas :20ohm');

end.
